{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": ["./src/resources/data/BankNumbers.ts"]
    },
    "jsRules": {
        
    },
    "rules": {
        "semicolon": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "interface-over-type-literal": false,
        "ordered-imports": false,
        "curly": [
            true,
            "ignore-same-line"
        ],
        "indent": [
            true,
            "spaces",
            2
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "trailing-comma": [
            false
        ]
    },
    "rulesDirectory": []
}